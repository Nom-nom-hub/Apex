    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "crypto-browserify": "^3.12.1",
    "esbuild": "^0.25.9",
    "eslint": "^9.34.0",
